# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file do not remove this or the tool will go crazy and scan the whole drive
root = true

[*]
indent_style = space
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.{js,cjs,mjs,ts,tsx}]
indent_size = 2
